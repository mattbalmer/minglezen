{
    "name": "MingleZen",
    "version": "0.3.1",
    "description": "Improves the interface of Mingle, providing a more elegant User Experience.",
    "manifest_version": 2,
    "permissions": [
        "http://*/*",
        "https://*/*",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "build/minglezen.min.js"
            ],
            "css": [
                "build/minglezen.min.css"
            ],
            "run_at": "document_start"
        }
    ]
}